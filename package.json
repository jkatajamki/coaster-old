    "build:server": "babel -d ./backend/dist/ ./backend/ -s",
    "build": "npm run build:clean && npm run build:client && npm run build:server",
    "dev:server": "nodemon --exec babel-node backend/server.js",
    "dev:client": "webpack-dev-server",
    "start:server": "node backend/dist/server.js"
  },
