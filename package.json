{
  "name": "coaster",
  "version": "1.0.0",
  "description": "Coaster.",
  "scripts": {
    "compose": "docker-compose -f docker-compose.yml up",
    "lint": "eslint --ext js --ext jsx .",
    "lint:fix": "eslint --ext js --ext jsx --fix .",
    "test:unit": "jest",
    "test-backend": "mocha backend/**/*.test.js --require @babel/register",
    "cypress:open": "cypress open",
